// Load two constants on Local Register 1 and 2
LDV L_REG01, 10 // [01] [000] [0000 0000 0000 1010] [000]
LDV L_REG02, 15 // [01] [001] [0000 0000 0000 1111] [000]

// Select RAM[0]
LDV ADDR_REG01, 0 // [01] [010] [0000 0000 0000 0000] [000] 

// store 10 + 15 in RAM[0]
// Vi_REG1 = L_REG01 + L_REG02 // [10] [100] [000] [001] [(0)000] [0000 0000 0] não funciona
// peculiaridade são da para salvar o valor de uma computação diretamente na RAM ouy VRAM 
// você precisa primeiro salvar em um registrador buffer e depois fazer um SWP

CMP L_REG02 = L_REG01 + L_REG02 // 100010000010000000000000
LD Vi_REG1, L_REG02         // 00 100 001 0000000000000000
JMP JUN, 0 // 11 11 0000000000000000000
/*

010000000000000001010000 -> 0x400050
010010000000000001111000 -> 0x480078
010100000000000000000000 -> 0x500000
// 101000000010000000000000 -> 0xA02000
100010000010000000000000 -> 0x882000
001000010000000000000000 -> 0x210000
111100000000000000000000 -> 0xF00000

*/
0x000a

